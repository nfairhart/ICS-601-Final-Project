# PostgreSQL/Supabase Database Configuration
user=your_db_user
password=your_db_password
host=your_db_host
port=5432
dbname=your_db_name

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
BUCKET_NAME=pdfs

# ChromaDB Configuration
CHROMA_PERSIST_DIR=./chroma_data

# OpenAI API Configuration (for PydanticAI)
OPENAI_API_KEY=sk-proj-your-openai-api-key

# CORS Configuration
# Comma-separated list of allowed origins (no trailing slashes)
# For development, include both localhost and 127.0.0.1
# For production, only include your production domain(s)
ALLOWED_ORIGINS=http://localhost:5001,http://127.0.0.1:5001
